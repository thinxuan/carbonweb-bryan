storage/logs/*
storage/framework/cache/*
storage/framework/sessions/*
storage/framework/views/*
!storage/.gitignore

# Testing
tests/
.phpunit.result.cache

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS
.DS_Store
Thumbs.db

# Local environment files
.env
.env.local
.env.*.local
.env.backup

# Build artifacts
*.log
npm-debug.log*

# Documentation
*.md
!README.md
google-cloud-deployment.md

